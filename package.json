{
  "name": "builly",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "start-dev": "node ./bin/www",
    "example": "cross-env npm run start-dev --config ./docs/demo-dirs/example.builders.json",
    "nodemon": "nodemon -i builders.json -i ./docs/demo-dirs/example.builders.json --exec npm run example",
    "build": "webpack --progress",
    "build:no-progress": "webpack",
    "build:prod": "npm run build -- --env.env=prod",
    "watch": "npm run build -- --watch",
    "watch:prod": "npm run watch -- --env.env=prod",
    "travis": "npm run build:prod && npm run example -- --travistest"
  },
  "dependencies": {
    "ansi-to-html": "^0.6.4",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-ws-routes": "^1.1.0",
    "hbs": "^4.0.0",
    "helmet": "^3.12.1",
    "http-errors": "~1.6.2",
    "linerstream": "^0.2.0",
    "material-design-icons-iconfont-only": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "minimist": "^1.2.0",
    "morgan": "~1.9.1",
    "sass-loader": "^7.0.3",
    "socket.io": "^2.1.1",
    "tree-kill": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.0",
    "daemonite-material": "^4.1.1",
    "daemonite-material-additions": "^1.0.2",
    "debounce": "^1.1.0",
    "file-loader": "^1.1.11",
    "jquery": "^3.4.0",
    "node-sass": "^4.13.1",
    "pickadate": "^3.5.6",
    "postcss-loader": "^2.1.5",
    "stein": "^1.0.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-watched-glob-entries-plugin": "^2.1.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie 10"
        ]
      }
    }
  }
}
